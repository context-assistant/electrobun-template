{
  "name": "bun-react-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "bun scripts/patch-electrobun-url-join.ts",
    "dev": "bun run dev:app",
    "dev:web": "bun --hot src/dev.ts",
    "build": "bun run build.ts",
    "build:landing": "bun scripts/build-landing.ts",
    "preview": "bun x serve dist",
    "preview:landing": "bun x serve landing",
    "dev:app": "bunx electrobun build --env=dev && bun scripts/open-electrobun-app.ts --env=dev",
    "build:app": "bunx electrobun build --env=stable",
    "generate:icons": "bun scripts/generate-app-icons.ts",
    "prettier": "prettier --write .",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@reduxjs/toolkit": "^2.11.2",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.545.0",
    "react": "^19",
    "react-dom": "^19",
    "react-redux": "^9.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "electrobun": "0.8.0",
    "happy-dom": "^18.0.1",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0"
  }
}
